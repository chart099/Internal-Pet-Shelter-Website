<form class="form add-cat-form">
        <label for="cat-name">Name: </label>
        <input id="cat-name" type="text">
        <label for="cat-breed">Breed: </label>
        <input id="cat-breed" type="text">
        <label for="cat-age">Age: </label>
        <input id="cat-age" type="text">
        <label for="cat-description">Description: </label>
        <input id="cat-description" type="text">
        <label for="cat-photo">Photo file: </label>
        <input type="submit">
         <input type="file" id="cat-photo" name="cat-photo">
        <label for="adoptable">Is this cat adoptable: </label>
        <input id="adoptable" type="checkbox">
        <button id="add-cat-btn" class="btn btn-primary" type="submit">Add Cat</button>
    </form>